-- (id) copyWithZone:(NSZone *)aZone
-{
-    ANTLRFastQueue *copy;
-
-    copy = [[[self class] allocWithZone:aZone] init];
-    copy.data = [data copyWithZone:nil];
-    copy.p = p;
-    copy.range = range;
-    return copy;
