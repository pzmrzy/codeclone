+#import <Cocoa/Cocoa.h>
+#import "AMutableArray.h"
+
+@interface ANTLRStreamEnumerator : NSEnumerator
+{
+	NSInteger i;
+	id eof;
+	AMutableArray *nodes;
+}
+
+-(id) initWithNodes:(AMutableArray *) n andEOF:(id) obj;
+-(BOOL) hasNext;
+
