+{ Antlr3Test }
+
+IDENTIFIER:
+        ('a'..'z'|'A'..'Z'|'_')
+        ('a'..'z'
+        |'A'..'Z'
+        |'0'..'9'
+        |'_'
+            {
+              std::cout << "Underscore";
+              std::cout << "foo";
+            }
+        )*
+    ;
+
+WS: (' ' | '\n')+;
diff --git a/runtime/Cpp/tests/t012.cpp b/runtime/Cpp/tests/t012.cpp
new file mode 100644
index 0000000..fe9d0c9
--- /dev/null
+++ b/runtime/Cpp/tests/t012.cpp
