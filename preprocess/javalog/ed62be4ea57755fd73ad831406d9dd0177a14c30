INT : '0'..'9'+;
-            WS : (' '|'\n') {$channel=HIDDEN;} ;
+            WS : (' '|'\n') {$channel=HIDDEN} ;
''')

found = self.execParser(grammar, "a", "int a:1,b:2,c:3;")
-        self.assertEquals("(int a 1) (int b 2) (int c 3)", found)
+        self.assertEqual("(int a 1) (int b 2) (int c 3)", found)


