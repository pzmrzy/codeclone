-        self.assertEquals(["line 1:3 missing INT at '<EOF>'"], errors)
-        self.assertEquals("abc <missing INT>", found)
+        self.assertEqual(["line 1:3 missing INT at '<EOF>'"], errors)
+        self.assertEqual("abc <missing INT>", found)


