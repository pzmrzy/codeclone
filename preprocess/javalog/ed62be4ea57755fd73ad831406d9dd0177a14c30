-            options {language=Python;output=AST;}
+            options {language=Python3;output=AST;}
a : ID INT ; // follow is EOF
ID : 'a'..'z'+ ;
INT : '0'..'9'+;
-            WS : (' '|'\n') {$channel=HIDDEN;} ;
+            WS : (' '|'\n') {$channel=HIDDEN} ;
''')

