+                "x   : ( options {greedy=true;}\n" +
+                "	   : y X\n" +
+                "      | y Y\n" +
+                "	   )\n" +
+                "    ;\n" +
+                "y   : L y R\n" +
+                "    | B\n" +
+                "    ;");
