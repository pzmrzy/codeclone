+namespace Antlr3.Tool
+{
+    // the various kinds of labels. t=type, id=ID, types+=type ids+=ID
+    public enum LabelType
+    {
+        None,
+        Rule,
+        Token,
+        RuleList,
+        TokenList,
+        // used in lexer for x='a'
+        Char,
+        // Used in tree grammar x=.
+        WildcardTree,
+        // Used in tree grammar x+=.
+        WildcardTreeList
+    }
+}
