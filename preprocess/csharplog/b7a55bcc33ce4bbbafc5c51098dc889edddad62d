@@ -1314,6 +1324,10 @@ namespace Antlr4.StringTemplate
invokedST.RawSetAttribute(arg.Name, defaultArgST);
}
}
+                else
+                {
+                    invokedST.RawSetAttribute(arg.Name, arg.DefaultValue);
+                }
}
}

